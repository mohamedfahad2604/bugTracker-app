package com.bizz.hyper.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class EcommerceController {

	@RequestMapping("/ecommerceProducts")
	public String ecommerceProducts() {
		return "apps-ecommerce-products";
	}

	@RequestMapping("/ecommerceProductsDetails")
	public String ecommerceProductsDetails() {
		return "apps-ecommerce-products-details";
	}

	@RequestMapping("/ecommerceOrders")
	public String ecommerceOrders() {
		return "apps-ecommerce-orders";
	}

	@RequestMapping("/ecommerceOrdersDetails")
	public String ecommerceOrdersDetails() {
		return "apps-ecommerce-orders-details";
	}

	@RequestMapping("/ecommerceCustomers")
	public String ecommerceCustomers() {
		return "apps-ecommerce-customers";
	}

	@RequestMapping("/ecommerceShoppingCart")
	public String ecommerceShoppingCart() {
		return "apps-ecommerce-shopping-cart";
	}

	@RequestMapping("/ecommerceCheckout")
	public String ecommerceCheckout() {
		return "apps-ecommerce-checkout";
	}

	@RequestMapping("/ecommerceSellers")
	public String ecommerceSellers() {
		return "apps-ecommerce-sellers";
	}

}
